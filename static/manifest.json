{
  "name": "Touzaisen",
  "version": "0.0.1",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": ["activeTab", "http://api.search.nicovideo.jp/"],
  "browser_action": {
    "default_title": "Touzaisen",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
